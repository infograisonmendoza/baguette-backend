{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "Menu item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "minLength": 3,
      "required": true
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "biginteger"
    },
    "photos": {
      "type": "component",
      "component": "shared.profile",
      "repeatable": true,
      "min": 1
    },
    "quantity": {
      "type": "integer"
    },
    "total_price": {
      "type": "biginteger"
    },
    "profile_image": {
      "type": "component",
      "component": "shared.profile",
      "repeatable": false
    },
    "observation": {
      "type": "text"
    },
    "options": {
      "type": "component",
      "component": "dish-props.dish-option",
      "repeatable": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "menu_items"
    }
  }
}
